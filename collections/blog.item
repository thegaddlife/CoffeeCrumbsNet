{.section item}

  <article class="{@|item-classes}" id="article-{id}" data-item-id="{id}">

	<div class="content-wrapper">

    <div class="post{.passthrough?} link-list-item{.end}">

      <!--POST IMAGE -->
      {.main-image?}
      <div class="sqs-block image-block sqs-block-image" data-block-type="5">
        <div class="sqs-block-content">
          <div class="image-block-outer-wrapper">
            <div class="intrinsic" style="max-width:800.0px;">
              <div style="padding-bottom:66.625%;" class="image-block-wrapper has-aspect-ratio" data-description="">
                <noscript>&lt;img src="{assetUrl}"  /&gt;</noscript>
                <img class="thumb-image loaded" data-src="{assetUrl}" data-image="{assetUrl}" data-image-dimensions="800x533" data-image-focal-point="0.5,0.5" data-load="false" data-image-id="55dfb90ee4b0eeba877176e9" data-type="image" src="{assetUrl}?format=2500w" data-image-resolution="2500w" style="top: -0.0318418560606091%; left: 0%; width: 100%; height: 100.063683712121%; position: absolute;">
              </div>
            </div>
          </div>
        </div>
      </div>
      {.end}

    <!--POST HEADER-->

			<header>

          <span class="article-dateline-above-title">
            <span class="date"><a href="{fullUrl}"><time class="published" datetime="{addedOn|date %F}">{addedOn|date %B %d, %Y}</time></a></span>
          </span>

          {.if title}<h1 class="entry-title" data-content-field="title">{.passthrough?}<a href="{sourceUrl}" target="_blank">{title}<span class="passthrough">&rarr;</span></a>{.or}<a href="{fullUrl}">{title}</a>{.end}</h1>{.end}

          <span class="article-dateline">
            <span class="date"><a href="{fullUrl}"><time class="published" datetime="{addedOn|date %F}">{addedOn|date %B %d, %Y}</time></a><span class="delimiter">/</span></span>
          </span>
          <span class="article-byline">
            {.section location}<span class="location">{.if addressTitle}<a href="//maps.google.com/maps?q={mapLat},{mapLng}" target="_blank" title="View map in new window">{addressTitle}</a><span class="delimiter">/</span>{.end}</span>{.end}
            {.section author}<span class="author"><a href="{collection.fullUrl}?author={id}" rel="author">{displayName}</a></span>{.end}
          </span>

        </header>

    <!--POST BODY-->

      {.section body}<div class="body entry-content">{@}</div>{.end}

    <!--POST FOOTER-->

    <!--OTHER POSTS BY CATEGORY -->
    <div class="sqs-block summary-v2-block sqs-block-summary-v2" data-block-json="{&quot;collectionId&quot;:&quot;5313bd5ee4b06c7ef09c8d67&quot;,&quot;design&quot;:&quot;carousel&quot;,&quot;headerText&quot;:&quot;More Perspective&quot;,&quot;textSize&quot;:&quot;small&quot;,&quot;pageSize&quot;:10,&quot;imageAspectRatio&quot;:&quot;1.5&quot;,&quot;columnWidth&quot;:270,&quot;gutter&quot;:60,&quot;listImageSize&quot;:30,&quot;listImageAlignment&quot;:&quot;left&quot;,&quot;slidesPerRow&quot;:5,&quot;textAlignment&quot;:&quot;left&quot;,&quot;showTitle&quot;:true,&quot;showThumbnail&quot;:true,&quot;showExcerpt&quot;:false,&quot;showReadMoreLink&quot;:false,&quot;showPrice&quot;:true,&quot;productQuickViewEnabled&quot;:false,&quot;showPastOrUpcomingEvents&quot;:&quot;Upcoming&quot;,&quot;metadataPosition&quot;:&quot;below-content&quot;,&quot;primaryMetadata&quot;:&quot;author&quot;,&quot;secondaryMetadata&quot;:&quot;none&quot;,&quot;filter&quot;:{&quot;category&quot;:&quot;Perspective&quot;},&quot;autoCrop&quot;:true,&quot;lightbox&quot;:false,&quot;mixedContent&quot;:true,&quot;blockId&quot;:&quot;4c6b3a3869163b787a79&quot;,&quot;hSize&quot;:null,&quot;floatDir&quot;:null}" data-block-type="55">

      <div class="sqs-block-content">

        <div class="

          summary-block-wrapper

          summary-block-collection-type-blog

          summary-block-setting-text-size-small
          summary-block-setting-text-align-left

          summary-block-setting-design-carousel
          summary-block-setting-design-list-thumbnail-left

          summary-block-setting-metadata-position-below-content
          summary-block-setting-primary-metadata-author
          summary-block-setting-secondary-metadata-none

          summary-block-setting-show-thumbnail
          summary-block-setting-show-title
          summary-block-setting-show-price
          summary-block-setting-hide-excerpt

          sqs-gallery-design-carousel
          sqs-gallery-design-carousel-slides-in-view-2

        ">

          <div class="summary-item-list-container sqs-gallery-container">

            <header class="summary-block-header">

              <!-- Collection Title -->
              <div class="summary-heading">

                  <span class="summary-header-text">More from category</span>

              </div>

              <!-- Carousel Nav -->
              <div class="summary-carousel-pager sqs-gallery-controls">
                <span class="summary-carousel-pager-next next"></span>
                <span class="summary-carousel-pager-prev previous sqs-disabled"></span>
              </div>

            </header>

            <div class="summary-item-list sqs-gallery sqs-gallery-design-carousel" style="width: 628px; transform: translateX(0px);">

            <!-- START QUERY HERE -->
            <squarespace:query collection="blog" limit="2">
                {.repeated section items}

                <div class="summary-item
                  summary-item-record-type-text
                  sqs-gallery-design-carousel-slide
                  summary-item-has-thumbnail

                  summary-item-has-cats
                  summary-item-has-tags
                  summary-item-has-author
                  summary-item-has-comments-enabled
                  positioned sqs-active-slide" data-type="image" style="width: 211px; padding-right: 6px; padding-left: 6px;">

                  <!-- Thumbnail -->
                  <a href="/blog/2015/8/28/preschool-and-perspective" class="summary-thumbnail-container sqs-gallery-image-container" data-title="preschool and perspective. " data-description="">
                    <div class="summary-thumbnail img-wrapper" style="overflow: hidden; padding-bottom: 66.6666666666667%;">

                      <!-- Main Image -->
                      <img data-src="http://static1.squarespace.com/static/5313b7c3e4b08cb68817cf91/5313bd5ee4b06c7ef09c8d67/55df8901e4b05ce5f41dbd3e/1440968298567/" data-image="http://static1.squarespace.com/static/5313b7c3e4b08cb68817cf91/5313bd5ee4b06c7ef09c8d67/55df8901e4b05ce5f41dbd3e/1440968298567/" data-image-dimensions="800x533" data-image-focal-point="0.5,0.5" data-load="false" class="summary-thumbnail-image loaded" alt="preschool and perspective. " src="http://static1.squarespace.com/static/5313b7c3e4b08cb68817cf91/5313bd5ee4b06c7ef09c8d67/55df8901e4b05ce5f41dbd3e/1440968298567/?format=500w" style="top: 0px; left: 0px; width: 200px; height: 133px; position: absolute;" data-image-resolution="500w">

                    </div>
                  </a>

                  <div class="summary-content sqs-gallery-meta-container">

                    <!-- Metadata (Below Content) -->
                    <div class="summary-metadata-container summary-metadata-container--below-content">

                      <div class="summary-metadata summary-metadata--primary">

                      	<!-- Author -->
                      	<span class="summary-metadata-item summary-metadata-item--author"><a href="/?author={authorId}#show-archive">{author.displayName}</a></span>

                      </div>

                    </div>

                  </div> <!-- End .summary-content -->

                </div> <!-- End .summary-item -->

                {.end}
                </squarespace:query>

            </div> <!-- End .summary-item-list -->

          </div> <!-- End .summary-item-list-container -->

        </div>

      </div>

    </div>

      <footer class="article-meta">
          <div>

            <span class="article-dateline">
              <span class="date"><a href="{fullUrl}"><i class="icon-calendar"></i><time class="published" datetime="{addedOn|date %F}">{addedOn|date %B %d, %Y}</time></a><span class="delimiter">/</span></span>
            </span>
            <span class="article-byline">
              {.section location}<span class="location">{.if addressTitle}<a href="//maps.google.com/maps?q={mapLat},{mapLng}" target="_blank" title="View map in new window"><i class="icon-map-marker"></i>{addressTitle}</a><span class="delimiter">/</span>{.end}</span>{.end}
              {.section author}<span class="author"><a href="{collection.fullUrl}?author={id}" rel="author"><i class="icon-user"></i>{displayName}</a><span class="delimiter">/</span></span>{.end}
            </span>

            {.comments?}
              {.if websiteSettings.disqusShortname}
              <span class="comments"><a href="{fullUrl}" class="comment-link"><i class="icon-comment"></i>{@|comment-link}</a></span>
              {.or}
              <span class="comments"><a href="{fullUrl}#commenting"><i class="icon-comment"></i>{.section publicCommentCount}{@} Comment{@|pluralize}{.or}Comment{.end}</a></span>
              {.end}
            {.end}

            {.section sourceUrl}<span class="source"><span class="delimiter">/</span><a href="{sourceUrl}" target="_blank"><i class="icon-share"></i>Source</a></span>{.end}

            {.section categories}<div class="categories"><i class="icon-bookmark"></i>{.repeated section @}<a href="{collection.fullUrl}?category={@|url-encode}" rel="tag">{@}</a>{.alternates with}, {.end}</div>{.end}
            {.section tags}<div class="tags"><i class="icon-tag"></i>{.repeated section @}<a href="{collection.fullUrl}?tag={@|url-encode}" rel="tag">{@}</a>{.alternates with}, {.end}</div>{.end}

            <div class="shareLoveButtons">
              {@|like-button}
              {@|social-button-inline}
            </div>

            {@|apply author-block.block}

            <div class="post-entry-injection clear-fix">{postItemInjectCode}</div>

          </div>
        </footer>

    </div><!-- /post -->

  </div><!-- /content-wrapper -->
  </article>


{.end}

<!--PAGINATION-->

{.section pagination}
<nav class="item pagination">
  <div class="content-wrapper">
    <ul>
      {.section prevItem}<li class="prev"><a href="{fullUrl}" title="{title}" id="prevLink"><i class="icon-chevron-left"></i><span class="prev-title">{title|truncate 36}</span></a></li>
      {.or}<li class="no-prev"><a href="{collection.fullUrl}"><i class="icon-list"></i>{collection.title}</a></li>{.end}
      {.section nextItem}<li class="next"><a href="{fullUrl}" title="{title}" id="nextLink"><span class="next-title">{title|truncate 36}</span><i class="icon-chevron-right"></i></a></li>
      {.or}<li class="no-next"><a href="{collection.fullUrl}"><i class="icon-list"></i>{collection.title}</a></li>{.end}
    </ul>
  </div>
</nav>
{.end}

<!--COMMENTS-->

{.section item}
  {.comments?}
  <div id="commenting">
    <section id="comments-section" class="comments-wrapper">
      {@|comments}
    </section>
  </div>
  {.end}
{.end}
